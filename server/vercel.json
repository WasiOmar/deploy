{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "MONGO_URL": "@mongo_url",
    "JWT_SECRET": "@jwt_secret",
    "GROQ_API_KEY": "@groq_api_key"
  }
}
